$(".newComment").dialog({
  autoOpen: true,
  height: 600,
  width: 600,
  modal: true,
  title: 'Add Comment',
  buttons: {
    "Edit ADD": function() { $("#<%= @comment.photo_id%>").submit() },
  },
  open: function() {
    $(".newComment").html("<%= escape_javascript(render :partial => 'form_comment') %>")
  },
});