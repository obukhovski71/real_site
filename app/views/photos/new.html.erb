<div class="photo new">
	<h2>Add photo</h2>
	<% debug @photo %>
	<%= form_for(:photo, :url => {:action => 'create', :user_id => params[:user_id]}) do |f|%>
	<table summary = "Photo form fields">
		<tr>
			<th>Title</th>
			<td><%= f.text_field(:title) %></td>
		</tr>
		<tr>
			<th>Description</th>
			<td><%= f.text_field(:description) %></td>
		</tr>
		<tr>
			<th>Date</th>
			<td><%= f.text_field(:date) %></td>
		</tr>
		<tr>
			<th>Upload image</th>
			<td><%= f.file_field :c_image%></td>
		</tr>
		<tr>
			<th>Uploaded by(user)</th>
			<td></td>
		</tr>
		</table>
		<div class="form-button">
			<%= submit_tag("Upload Photo")	%>
		</div>
		<%end%>
		</div>
